class Solution:
    def longestValidParentheses(self, s: str) -> int:
        if not s: return 0
        stack = [-1]
        res = 0
        for i in range(len(s)):
            if s[i] is '(':
                stack.append(i)
            else:
                stack.pop()
                if stack:
                    res = max(i - stack[-1], res)
                else:
                    stack.append(i)
        return res



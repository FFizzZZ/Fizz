class Solution:
    def multiply(self, num1: str, num2: str) -> str:
        res = 0
        for i in range(len(num1)-1, -1, -1):
            for j in range(len(num2)-1, -1, -1):
                a = (ord(num1[i]) - ord('0')) * 10 ** (len(num1) - 1 -i)
                b = (ord(num2[j]) - ord('0')) * 10 ** (len(num2) - 1 - j)
                res += a * b
        return str(res)
                

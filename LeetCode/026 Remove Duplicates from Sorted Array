class Solution:
    def removeDuplicates(self, nums: 'List[int]') -> 'int':
        if not nums: return 0
        count = 1
        n = len(nums)
        t = nums[:]
        for i in range(1, n):
            if t[i] == t[i-1]:
                nums.remove(t[i])
                continue
            else:
                count += 1
        return count


###
class Solution:
    def removeDuplicates(self, nums: 'List[int]') -> 'int':
        if not nums:
            return 0

        newTail = 0

        for i in range(1, len(nums)):
            if nums[i] != nums[newTail]:
                newTail += 1
                nums[newTail] = nums[i]

        return newTail + 1

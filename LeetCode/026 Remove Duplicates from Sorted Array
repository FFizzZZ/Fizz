class Solution:
    def removeDuplicates(self, nums: 'List[int]') -> 'int':
        if not nums: return 0
        count = 1
        n = len(nums)
        t = nums[:]
        for i in range(1, n):
            if t[i] == t[i-1]:
                nums.remove(t[i])
                continue
            else:
                count += 1
        return count


###     
class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        idx = 0
        for num in nums:
            if idx < 1 or num != nums[idx-1]:
                nums[idx] = num
                idx += 1
        return idx

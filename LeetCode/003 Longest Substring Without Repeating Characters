class Solution:
    def lengthOfLongestSubstring(self, s: 'str') -> 'int':
        sub = ''
        res = ''
        for i in s:
            if not i in sub:
                sub += i
            else:
                if len(res) < len(sub):
                    res = sub
                sub = sub.split(i)[-1] + i
        return max(len(sub), len(res))
